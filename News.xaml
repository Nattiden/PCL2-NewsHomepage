	<local:MyCard Margin="0,0,0,15" Title="最新正式版 - 1.18.2" CanSwap="True" IsSwaped="True">
		<!--Header-->
		<StackPanel Margin="30,40,30,20">
			<Border Style="{StaticResource verImgBdr}">
				<Border.Background>
					<ImageBrush ImageSource="https://www.lightbeacon.top/pnh/newimgs/1_18/22w07a.png" Stretch="Uniform" />
				</Border.Background>
				<Image Source="https://www.lightbeacon.top/pnh/newimgs/1_18/22w07a.png" Opacity="0" />
			</Border>
			<Border Style="{StaticResource verTxtBdr}">
				<TextBlock Style="{StaticResource verTxtBlk}" Text="1.18.2" />
			</Border>
			<!--Detail-->
			<TextBlock Text="新内容 ＆ 更改" Style="{StaticResource h2}" />
				<TextBlock Text="方块" Style="{StaticResource h3}" />
					<StackPanel Style="{StaticResource ListStyle}" Margin="60,10,0,10">
						<Ellipse />
						<TextBlock Style="{StaticResource ListItem}" Text="末影箱现在不会在圣诞节期间使用圣诞礼物纹理。" />
					</StackPanel>
				<TextBlock Text="生物" Style="{StaticResource h3}" />
					<StackPanel Style="{StaticResource ListStyle}" Margin="60,10,0,10">
						<Ellipse />
						<TextBlock Style="{StaticResource ListItem}" Text="海豚会更准确地选择离其最近的结构，而无论结构的类型如何。" />
					</StackPanel>
				<TextBlock Text="世界生成" Style="{StaticResource h3}" />
					<StackPanel Style="{StaticResource ListStyle}" Margin="60,10,0,10">
						<Ellipse />
						<TextBlock Style="{StaticResource ListItem}" Text="堡垒遗迹稍微提高了结构边界箱的准确度，现在会更贴近结构本身的形状。" />
						<Ellipse />
						<TextBlock Style="{StaticResource ListItem}" Text="worldgen结构中以前只能使用元素ID列表的一些字段现在可以使用标签。" />
						<Ellipse />
						<TextBlock Style="{StaticResource ListItem}" Text="一些标签字段现在要求在ID前面加上 # ，但是它们暂不接受元素列表。" />
						<Ellipse />
						<TextBlock Style="{StaticResource ListItem}" Text="噪声设置中的结构字段不再将要塞与非要塞结构相独立，每种结构在JSON中的级别现在都是平等的，用其中的type指定结构的类型。" />
						<Ellipse />
						<TextBlock Style="{StaticResource ListItem}" Text="数字0的种子不再按照特殊情况处理。" />
						<Ellipse />
						<TextBlock Style="{StaticResource ListItem}" Text="输入的种子前后的任何空格现在都会被去掉。" />
						<Ellipse />
						<TextBlock Style="{StaticResource ListItem}" Text="将超平坦世界“红石俱备”预设的顶层方块高度由Y=-9提升至Y=55，以防止史莱姆生成。" />
						<Ellipse />
						<TextBlock Style="{StaticResource ListItem}" Text="更改了超平坦世界“水世界”预设，将原先的沙子替换成沙砾，并在基岩层上方增加了64层深板岩，以解决此预设世界中海底神殿的生成问题。" />
						<Ellipse />
						<TextBlock Style="{StaticResource ListItem}" Text="自定义世界生成在已配置结构文件中加入了biomes字段，值是一个标签。" />
						<Ellipse />
						<TextBlock Style="{StaticResource ListItem}" Text="自定义世界生成加入了样条密度函数：一种通用的放置方块的方法，可使用户通过其表达几乎所有的使用三次样条的函数。" />
					</StackPanel>
				<TextBlock Text="命令格式" Style="{StaticResource h3}" />
					<StackPanel Style="{StaticResource ListStyle}" Margin="60,10,0,10">
						<Ellipse />
						<TextBlock Style="{StaticResource ListItem}" Text="加入了 /placefeature，更改了 /locate 和 /locatebiome。" />
