	<local:MyCard Margin="0,0,0,15" Title="最新预览版 - 1.19-pre1" CanSwap="True">
		<!--Header-->
		<StackPanel Margin="30,40,30,20">
			<Border Style="{StaticResource verImgBdr}">
				<Border.Background>
					<ImageBrush ImageSource="https://www.lightbeacon.top/pnh/newimgs/1_19/1.19-pre1.png" Stretch="Uniform" />
				</Border.Background>
				<Image Source="https://www.lightbeacon.top/pnh/newimgs/1_19/1.19-pre1.png" Opacity="0" />
			</Border>
			<Border Style="{StaticResource verTxtBdr}">
				<TextBlock Style="{StaticResource verTxtBlk}" Text="1.19-pre1" />
			</Border>
			<!--Detail-->
			<StackPanel>
				<StackPanel.Resources>
					<Style TargetType="Image">
						<Setter Property="MaxWidth" Value="70px" />
						<Setter Property="MaxHeight" Value="70px" />
						<Setter Property="Margin" Value="3" />
					</Style>
					<Style TargetType="Border">
						<Setter Property="Background" Value="{DynamicResource ColorBrush7}" />
						<Setter Property="BorderBrush" Value="{DynamicResource ColorBrush3}" />
						<Setter Property="BorderThickness" Value="2px" />
						<Setter Property="CornerRadius" Value="5" />
						<Setter Property="Margin" Value="2px" />
					</Style>
					
					<Style TargetType="TextBlock">
						<Setter Property="HorizontalAlignment" Value="Center" />
						<Setter Property="VerticalAlignment" Value="Center" />
						<Setter Property="Grid.Row" Value="0" />
						<Setter Property="Foreground" Value="{DynamicResource ColorBrush7}" />
						<Setter Property="FontSize" Value="12px" />
						<Setter Property="Margin" Value="8px,2px" />
					</Style>
				</StackPanel.Resources>
				<!--Details-->
				<TextBlock Text="修改内容" Style="{StaticResource h2}" />
				<StackPanel Style="{StaticResource ListStyle}" Margin="60,10,0,10">
					<Ellipse />
					<TextBlock Style="{StaticResource ListItem}" Text="稍微下调了红树木沼泽中红树的生成数量" />
					<Ellipse />
					<TextBlock Style="{StaticResource ListItem}" Text="末影人，骷髅，凋灵骷髅和猪灵现在会在下界中更广的光照强度范围中生成（从光照强度等级0到11）" />
					<Ellipse />
					<TextBlock Style="{StaticResource ListItem}" Text="在开始或结束“使用”一个物品时，与物品交互会产生振动（例如弓、十字弩、山羊角、盾和食物）" />
					<Ellipse />
					<TextBlock Style="{StaticResource ListItem}" Text="现在潜行时与物品交互不会产生振动" />
					<Ellipse />
					<TextBlock Style="{StaticResource ListItem}" Text="在装备栏中装备非盔甲的物品（如南瓜和头颅）现在有单独的装备音效" />
				</StackPanel>

				<TextBlock Text="技术性修改" Style="{StaticResource h2}" />
				<StackPanel Style="{StaticResource ListStyle}" Margin="60,10,0,10">
					<Ellipse />
					<TextBlock Style="{StaticResource ListItem}" Text="自动补全现在可用于 place template 的模板参数" />
					<Ellipse />
					<TextBlock Style="{StaticResource ListItem}" Text="自定义服务器现在可以通过发送新的网络数据包的方式以对特定客户端启用或禁用聊天预览" />
					<Ellipse />
					<TextBlock Style="{StaticResource ListItem}" Text="现在，聊天预览在聊天相关指令中也会展示。例如 /say 和 /msg" />
					<Ellipse />
					<TextBlock Style="{StaticResource ListItem}" Text="test-rainbow-chat 从 server.properties 中移除了" />
				</StackPanel>

				<TextBlock Text="添加的游戏事件" Style="{StaticResource h3}" />
				<StackPanel Style="{StaticResource ListStyle}" Margin="60,10,0,10">
					<Ellipse />
					<TextBlock Style="{StaticResource ListItem}" Text="note_block_play 带有振动频率6" />
					<Ellipse />
					<TextBlock Style="{StaticResource ListItem}" Text="当它启动后，聊天框上将出现一个受服务器控制的预览界面，显示消息发送时的样子。" />
					<Ellipse />
					<TextBlock Style="{StaticResource ListItem}" Text="instrument_play 带有振动频率15" />
				</StackPanel>
			</StackPanel>
			<TextBlock Text="该版本修复了 44 个bug，本文来源于 MCBBS" Style="{StaticResource footnote}" />
			<!--Logs-->
			<GroupBox Header="阅读完整日志" BorderBrush="{DynamicResource ColorBrush3}">
				<Grid UseLayoutRounding="True" Margin="10,10,10,10">
					<Grid.ColumnDefinitions>
						<ColumnDefinition Width="20*"></ColumnDefinition>
						<ColumnDefinition Width="10*"></ColumnDefinition>
						<ColumnDefinition Width="10*"></ColumnDefinition>
					</Grid.ColumnDefinitions>
					<local:MyButton Style="{StaticResource Read0}" EventData="https://minecraft.fandom.com/zh/wiki/Java%E7%89%881.19-pre1" ColorType="Highlight" />
					<local:MyButton Style="{StaticResource Read1}" EventData="https://www.mcbbs.net/thread-1340927-1-1.html" />
					<local:MyButton Style="{StaticResource Read2}" EventData="https://www.minecraft.net/zh-hans/article/minecraft-1-19-pre-release-1" />
				</Grid>
			</GroupBox>	
		</StackPanel>	
	</local:MyCard>

